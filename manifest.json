{
  "manifest_version": 3,
  "name": "gmail Scraper",
  "version": "1.0",
  "description": "A simple Gmail scraper Chrome extension",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
